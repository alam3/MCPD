@page
@model NorthwindWeb.Pages.CustomersModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="row">
    <h1 class="display-2">Customers</h1>
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>Customer Name</th><th>Country</th></tr>
        </thead>
        <tbody>
            @foreach (SortedList<string,string> customer in Model.Customers) {
                <tr><td>@customer["CustomerName"]</td><td>@customer["Country"]</td></tr>
            }
        </tbody>
    </table>
</div>